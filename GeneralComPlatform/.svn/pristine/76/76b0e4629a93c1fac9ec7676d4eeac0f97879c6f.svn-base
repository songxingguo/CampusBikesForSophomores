<img ng-if="logined"
     id="user-avatar"
     class="img-circle"
     src="/img/avatar-light.png"
     ng-src="{{user.avatar}}"
     data-toggle="tooltip"
     data-placement="bottom"
     title="{{user.name}}"
     ng-click="goToCenter()">

<a id="login-out" class="btn btn-link btn-xs ng-scope" ng-click="logout()">退出</a>