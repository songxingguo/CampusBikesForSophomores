
.null-wrap{
  position: relative;
  top:0;
  left:0;
  width: 100%;
  height: 400px;
  background-size: cover;
  background-position: 50% 50%;

  canvas {
    display: block;
    vertical-align: bottom;
    position: absolute;
    top: 0;
    left: 0;
  }

  h1{
    padding-top: 150px;
    text-align: center;
    color: #777575;
  }


}
