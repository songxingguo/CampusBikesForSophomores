        margin-right: -1px;
    }
    .guide_name{
        color: #1d6093;
        font-size: 17px;
        /*background: red;*/
        width: auto;
         height: 100%;
    }
}
.breadcrumb > .active{
    color:#ffffff;
}
.login-head{
    clear: both;
