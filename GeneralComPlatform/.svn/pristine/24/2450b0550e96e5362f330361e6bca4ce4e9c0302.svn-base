/*-----------------------左边导航栏-------------------------*/
.divScroll{
    width: 232px;
    overflow: hidden;

    .nav_left{
    width: 252px;
    z-index:12;
        overflow-y: auto;
        overflow-x: hidden;

     height: 100%;  

    .nav_line{
        width: 232px;
        border-bottom: 1px solid #3c434c;
        cursor:pointer;


        img{
            margin-top:-5px;
            margin-right: 10px;
            margin-left: 28px;
        }

        .link_style{
            font-size: 18px;
            color: #a0aab2;
            width: 100%;
            height: 45px;
            width:100%;
            line-height:45px;
            padding666666-left: 18px;
        }

        .link_style:hover{
            /*background: #6cb7f1;*/
            color : #ffffff;
            /*width: 100%;
            /*width: 102%;*/
            background: url('img/hoverBack.png');
            background-repeat: no-repeat;
            background: #466785;
            position: relative;
        }
        .next_img{
            margin-right: -10px;
            margin-top: -30px;
            /* position: relative; */
            float: right;
        }

        ul{
            margin-bottom:0px;
            background: #5cbce1;
            li{
                list-style:none;
                font-size: 15px;
                height: 40px;
                background: #3c4550;
                color : #ffffff;
                line-height: 40px;

                .second_style{
                    height: 100%;
                    line-height: 40px;
                    margin-left:28%;
                    font-weight: 100;
                }
            }
            li:hover{
                background: #303841;
                color: #5cbce1;
                margin-left: 4px;
            }
        }
    }
}
}

/*-----------------------右侧内容---------------------------*/
.content_right{
    /*float: left;*/
    background: #e8eef3;
    /*width: 82%;*/
    padding: 7px 15px 7px 15px;
    margin-left:232px;
}

/*-----------------------中间的内容--------------------------*/
.mainBody{
    width: 100%;

    .left_content{
        width: 232px;
        float: left;
        position: fixed;
        background: #303841;
        height:100%;


        .head_portrait{
           /* padding: 14px;*/
            height:150px;
            border-bottom: 1px solid #3c434c;
            background-image: url("/img/headBcakground.png");
            background-repeat: no-repeat;

            .head_img{
                float: left;
            }
            .head_info{
                float: left;
                width: 109px;
                margin-top: 10px ;
                margin-left: 10px;

                .name{
                    margin: 0;
                    font-size: 17px;
                    color: #5cbce1;
                }
                .company{
                    margin: 12px 0 0 0;
                    color: #afb7be;
                    font-size: 12px;
                }
            }
        }
        .boottom_info{
            width: 232px;
            bottom: 0;
            position: fixed;
            bottom: 0;
            left: 0;
        }
    }
}

.bottom-content{
    /*width: 82%;*/
    margin-left: 232px;
}
.bottom-info{
    height: 50px;
    width: 232px;
    position: fixed;
    bottom: 0px;
    background: #303841 none repeat scroll 0 0;

    p{
        z-index: 13;
        font-size: 12px;
        color: #9e9e9e;
        margin-top: 0px;
        margin-left: 10px;
        margin-top:20px;
    }
}

a:hover{
    text-decoration:none;
}

.over_nav_true{
    background: #6cb7f1;
    color : #ffffff;

}

.over_nav_false{
    background: #303841;
    color: #a0aab2;
}

/*头部*/
.center_head{
    height: 50px;
    background: #ffffff;
    width: 100%;
    margin-bottom: 10px;

    .login_out{
        float: right;
        width: 127px;
        height: 100%;


        .button_out{
            height: 30px;
            background: #ff794e;
            border-radius: 5px;
            width: 84px;
            margin-top: 10px;
            margin-left: 28px;
            text-align: center;
            padding:5px;
            cursor: pointer;

            span{
                color:#ffffff;
                font-size: 14px;
            }
        }
    }

    ul{
        height:100%;
        width: 100%;
        float: left;

        li{
            width:33%;
            float: left;
            list-style: none;
            height:100%;

            .img_info{
                margin-left:16px;
                margin-top: 15px;
                float: left;
            }
            .img_infoCircle{
                margin-left:-10px;
                margin-top: 9px;
                float: left;
            }
            .info_num{
                color:#ffffff;
                font-size: 12px;
                margin-left:-15px;
                float: left;
                margin-top: 8px;
                text-align: center;
                width: 15px;
                height: 15px;
            }
            .info_num_detail{
                margin-top: 16px;
                font-size: 14px;
                color: #666666;
                margin-left: 9px;
                float: left;
            }
        }
    }
}
.nav_background{
    margin-top: -227px;height: 227px;width: 232px;position:relative;z-index: 11;
}

.crumb{
    height: 38px;
    background: #ffffff;
    width: 100%;

    .guide{
        width: 5px;
        background: #1d6093;
        height: 100%;
