<!--查看用户明细-->
<!--基本信息的区域-->
<form show-one>
    <div class="input-title">
        <div>
            <img src="img/eye.png"/>
        </div>
        <div style="width: 8%;text-align: center;">
            <span>用户登录信息</span>
        </div>
        <div style="width: 90%;" class="line-full">

        </div>
    </div>
    <table class="basicMass readDetail">
        <tr class="basicMass-input">
            <td class="key user_detail_key">
                <span>用户名</span>:
            </td>
            <td class="basicMass-input-td user_detail_td">
                <p class="show-content">{{userObject.userName}}</p>
            </td>

            <td class="key noFirst user_detail_key">
                <span>账号</span>:
            </td>
            <td class="basicMass-input-td user_detail_td">
                <p class="show-content">{{userObject.userNo}}</p>
            </td>
        </tr>
    </table>

    <div class="input-title">
        <div>
            <img src="img/eye.png"/>
        </div>
        <div style="width: 8%;text-align: center;">
            <span>用户基本信息</span>
        </div>
        <div style="width: 90%;" class="line-full">

        </div>
    </div>

    <table class="basicMass readDetail">
        <tr class="basicMass-input">
            <td class="key user_detail_key">
                <span>联系人电话</span>:
            </td>
            <td class="basicMass-input-td user_detail_td">
                <p class="show-content">{{userObject.userPhone}}</p>
            </td>

            <td class="key noFirst user_detail_key">
                <span>学院名称</span>:
            </td>
            <td class="basicMass-input-td user_detail_td">
                <p class="show-content">{{userObject.userCollege}}</p>
            </td>
        </tr>

        <tr class="basicMass-input">
            <td class="key">
                <span>邮箱</span>:
            </td>
            <td class="basicMass-input-td">
                <p class="show-content">{{userObject.userMailbox}}</p>
            </td>
            <td class="key noFirst">
                <span>状态</span>:
            </td>
            <td class="basicMass-input-td">
                <p class="show-content">{{state}}</p>
            </td>
        </tr>

        <!--<tr class="basicMass-input">
            <td class="key">
                <span>备注</span>:
            </td>

            <td class="basicMass-input-td" colspan="3">
                <p class="show-content remark_user">{{userObject.remark}}</p>
            </td>
        </tr>-->
    </table>
    <!--按钮区域-->
    <div class="page-operator">
        <a class="button white" ng-click="backBeforPage()">返回</a>
    </div>
</form>
