/*样式布局*/
.center-body {
    text-align: center;
}
.center-header {
    width: 100%;
    height: 100px;
    position: fixed;
    top: 0;
    border-bottom: solid 1px #ccc;
    background-color: #fff;
}
.center-header ul li {
    display: inline-block;
}
.logo {
    width: 20%;
    height: 100px;
    line-height: 100px;
}
.logo-font {
    /*font-family: "STHupo";*/
    font-family: "STCaiyun";
    font-size: 25px;
    font-weight: bold;
}
.center-nav {
    width: 85%;
}
.nav {
    width: 14%;
    height: 100px;
    line-height: 100px;
    font-size: 20px;
    font-weight: 500;
}
.center-nav-select {
    width: 20%;
    font-size: 15px;
    font-weight: bold;
}
.center-nav-select ul li {
    display: inline-block;
}
.login-content {
    width: 80%;
    height: 490px;
}
.login-logo {
    height: 260px;
}
.login-logo img {
    width: 266px;
    height: 260px;
}
.login-title {
    width: 400px;
    height: 50px;
    text-align: left;
    line-height: 50px;
    font-size: 16px;
    font-weight: 600;
}
.login-title ul li {
    display: inline;
}
.login-li-border {
    border-right: 2px solid #666;
    padding-left: 0px;
    padding-right: 4px;
}
.login-from {
    width: 400px;
    height: 160px;
    font-size: 14px;
}
.register-select {
    text-align: left;
}
.register-code-input {
    width: 70%;
}
.register-send-code {
    width: 30%;
    background-color: #1abb9c;
}
.login-forget {
    display: inline-block;
    float: right;
}
.center-footer {
    width: 100%;
}
.center-footer-content {
    width: 90%;
    height: 60px;
    font-size: 12px;
}
.center-footer-logo {
    width: 30%;
    height: 60px;
    margin-top: 10px;
}
.center-footer-right {
    width: 60%;
}
.center-footer-right ul li {
    display: inline-block;
}
.center-footer-link {
    width: 10%;
    height: 60px;
    line-height: 60px;
}
.center-footer-copyright {
    width: 65%;
}

/**效果**/
.center-nav .nav a {
    color: #000;
}
.center-nav .nav a:hover {
    color: #1abb9c;
    cursor: pointer;
}
.center-nav-select a{
    color: #666;
}
.center-nav-select a:hover {
    color: #1abb9c;
    cursor: pointer;
}
.login-title a {
    color: #666;
}
.login-title a:hover {
    color: #1abb9c;
    cursor: pointer;
}
.login-title a:active {
    color: #1abb9c;
}
.center-footer-right a {
    color: #666;
}
.center-footer-right a:hover {
    color: #337ab7;
    cursor: pointer;
}

/**弹框样式**/
.modal-content {
    min-height: 400px;
    border-radius: 0;
}
.modal-dialog {
    width: 700px;
}
.modal-header {
    height: 40px;
    padding: 10px;
    color: #1abb9c;
}
.model-search {
    width: 65%;
}
.model-search input {
    border: 1px solid #1abb9c;
    height: 35px;
}
.model-search button {
    width: 68px;
    color: #fff;
    background-color: #1abb9c;
    font-size: 14px!important;
    border: 1px solid #1abb9c;
    height: 35px;
}
.model-selected {
    color: #fff!important;
    background-color: #1abb9c;
    font-size: 14px;
    padding: 5px 10px;
    border-radius: 4px;
}
.model-line {
    border: 1px solid #ccc;
    margin: 25px 20px 0 20px;
    height: 1px;
}
.model-info-list {
    text-align: center;
}
.model-info-list ul li {
    width: auto;
    display: inline-block;
    margin: 5px 10px;
}
.register-send-code:hover, .register-send-code:active,.register-send-code:visited, .register-send-code:focus{
    background-color: #14977e;
    border: solid 1px #14977e;
}